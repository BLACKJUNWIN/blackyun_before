ath: \"/usr/blackjun/blackYun\",\n          size: item.size\n        });\n        const content = res\n        const blob = new Blob([content])\n        let fileName = item.name + \".zip\";\n        if ('download' in document.createElement('a')) { // 非IE下载\n          const elink = document.createElement('a')\n          elink.download = fileName\n          elink.style.display = 'none'\n          elink.href = URL.createObjectURL(blob)\n          document.body.appendChild(elink)\n          elink.click()\n          URL.revokeObjectURL(elink.href) // 释放URL 对象\n          document.body.removeChild(elink)\n        } else { // IE10+下载\n          navigator.msSaveBlob(blob, fileName)\n        }\n        this.$message({\n          type: 'success',\n          message: '正在下载!'\n        });\n      }\n    },\n    activeFile(item) {\n      if (this.$store.state.uploadSize === 0) {\n        this.downFile = item;\n      }\n      this.fileManage = true;\n    },\n    goHome() {\n      if (this.$store.state.uploadSize === 0) {\n        console.log(11);\n        window.open(this.downFile.path, '_blank')\n      } else {\n        this.$store.commit(\"tip\", {code: \"500\", message: \"当前任务正在进行\"})\n      }\n    }\n  },\n  data() {\n    return {\n      downFile: '',\n      downSize: '',\n      fileManage: false,\n      fileBlack: false,\n      speed: 0,\n      oldSize: {\n        size: 0,\n        time: 0\n      },\n      newSize: {\n        size: 0,\n        time: 0\n      },\n      simple: [\n        {\n          class: \"安全\",\n          title: \"互联万物,安全最大\",\n          classColor: \"color:#6dbb70\",\n          icon: require(\"@/assets/images/simple/safe.svg\"),\n          programs: [\n            {\n              name: \"Adguard\",\n              icon: require(\"@/assets/images/simple/adguard.svg\"),\n              path: \"https://adguard.com\",\n              filepath: \"/usr/blackjun/blackYun/极简软件/安全/adguard.zip\",\n              advantage: \"绝弹窗,免视频广告,不贵,功能十分强大\",\n              shortc