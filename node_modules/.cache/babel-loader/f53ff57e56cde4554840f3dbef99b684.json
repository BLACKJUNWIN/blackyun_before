CArCM;;IAsCP,MAAME,QAAN,CAAeT,IAAf,EAAoB;MAClB,IAAG,KAAKd,MAAL,CAAYC,KAAZ,CAAkBK,UAAlB,KAA+B,CAAlC,EAAoC;QAClC,KAAKN,MAAL,CAAYO,MAAZ,CAAmB,KAAnB,EAAyB;UAACC,IAAI,EAAC,KAAN;UAAYC,OAAO,EAAC;QAApB,CAAzB;MACD,CAFD,MAEM;QACJ,KAAK5B,QAAL,GAAgBiC,IAAhB;QACA,IAAIH,GAAE,GAAI,MAAMrC,IAAI,CAACwC,IAAD,CAApB;QACA,MAAMU,OAAM,GAAIb,GAAhB;QACA,MAAMc,IAAG,GAAI,IAAIC,IAAJ,CAAS,CAACF,OAAD,CAAT,CAAb;QACA,IAAIZ,QAAJ;;QACA,IAAIE,IAAI,CAACC,IAAL,KAAc,MAAd,IAAwBD,IAAI,CAAC9B,IAAL,GAAY,OAAK,IAAL,GAAU,EAAlD,EAAsD;UACpD4B,QAAO,GAAIE,IAAI,CAACF,QAAL,GAAgB,MAA3B;QACF,CAFA,MAEO;UACLA,QAAO,GAAIE,IAAI,CAACF,QAAL,GAAgB,GAAhB,GAAsBE,IAAI,CAACC,IAAtC;QACF;;QACA,IAAI,cAAcY,QAAQ,CAACC,aAAT,CAAuB,GAAvB,CAAlB,EAA+C;UAAE;UAC/C,MAAMC,KAAI,GAAIF,QAAQ,CAACC,aAAT,CAAuB,GAAvB,CAAd;UACAC,KAAK,CAACC,QAAN,GAAiBlB,QAAjB;UACAiB,KAAK,CAACE,KAAN,CAAYC,OAAZ,GAAsB,MAAtB;UACAH,KAAK,CAACI,IAAN,GAAaC,GAAG,CAACC,eAAJ,CAAoBV,IAApB,CAAb;UACAE,QAAQ,CAACS,IAAT,CAAcC,WAAd,CAA0BR,KAA1B;UACAA,KAAK,CAACS,KAAN;UACAJ,GAAG,CAACK,eAAJ,CAAoBV,KAAK,CAACI,IAA1B,EAP6C,CAOb;;UAChCN,QAAQ,CAACS,IAAT,CAAcI,WAAd,CAA0BX,KAA1B;QACF,CATA,MASO;UAAE;UACPY,SAAS,CAACC,UAAV,CAAqBjB,IAArB,EAA2Bb,QAA3B;QACF;;QACA,KAAK+B,QAAL,CAAc;UACZ5B,IAAI,EAAE,SADM;UAEZN,OAAO,EAAE;QAFG,CAAd;MAIF;IACD,CArEM;;IAsEPmC,UAAU,GAAE;MACV,IAAIC,UAAU,GAAC,iCAA+B,UAA/B,GAA0C,KAAKjE,WAAL,CAAiBe,IAAjB,CAAsBmD,SAAtB,CAAgC,KAAKlE,WAAL,CAAiBmE,QAAjB,CAA0BC,MAA1B,GAAiC,CAAjE,CAAzD;MACAC,MAAM,CAACC,IAAP,CAAYL,UAAZ,EAAwB,QAAxB;IACD,CAzEM;;IA0EP,MAAMxE,UAAN,GAAkB;MAChB,IAAIsC,GAAG,GAAC,MAAMtC,UAAU,CAAC,KAAKO,WAAN,CAAxB;MACA,KAAKoB,MAAL,CAAYO,MAAZ,CAAmB,KAAnB,EAAyBI,GAAzB;;MACA,IAAGA,GAAG,CAACH,IAAJ,KAAW,KAAd,EAAoB;QAClB,MAAM,KAAKhC,QAAL,CAAc,KAAKiB,MAAL,CAAYC,KAAZ,CAAkBC,IAAhC,CAAN;MACF;IACF;;EAhFO,CA3DI;EA6IbwD,QAAQ,EAAC,EA7II;;EAgJb,MAAMC,OAAN,GAAe;IACb,MAAM,KAAK5E,QAAL,CAAc,KAAKiB,MAAL,CAAYC,KAAZ,CAAkBC,IAAhC,CAAN;IACA,IAAI0D,IAAI,GAACzD,cAAc,CAAC0D,OAAf,CAAuB,SAAvB,CAAT;;IACA,IAAGD,IAAH,EAAQ;MACN,KAAKjE,OAAL,GAAaU,IAAI,CAACyD,KAAL,CAAWF,IAAX,CAAb;MACAzD,cAAc,CAAC4D,UAAf,CAA0B,SAA1B;IACF;EACD,CAvJY;;EAwJbC,OAAO,GAAE;IACP,IAAIR,MAAM,CAACS,OAAP,IAAkBT,MAAM,CAACS,OAAP,CAAeC,SAArC,EAAgD;MAC9CD,OAAO,CAACC,SAAR,CAAkB,IAAlB,EAAwB,IAAxB,EAA8BhC,QAAQ,CAACO,GAAvC;MACAe,MAAM,CAACW,gBAAP,CAAwB,UAAxB,EAAoC,KAAK1C,GAAzC,EAF8C,CAEA;IAChD;EACD;;AA7JY,CAAf","names":["deleteFile","down","newMkdir","showPack","name","data","currentPack","currentFile","downFile","speed","oldSize","size","time","newSize","currentPath","records","fileManage","addMkdir","mkdirName","watch","$route","query","path","sessionStorage","setItem","JSON","stringify","$store","state","uploadReading","Date","getTime","toFixed","uploadSize","commit","code","message","methods","res","fileName","openPack","item","type","push","$router","fun","pop","getType","require","e","fileDown","content","blob","Blob","document","createElement","elink","download","style","display","href","URL","createObjectURL","body","appendChild","click","revokeObjectURL","removeChild","navigator","msSaveBlob","$message","OnlineFile","onlinePath","substring","mainPath","length","window","open","computed","created","temp","getItem","parse","removeItem","mounted","history","pushState","addEventListener"],"sourceRoot":"","sources":["D:\\BlackJun\\2021\\program\\webstrom\\yun\\src\\views\\more.vue"],"sourcesContent":["<template>\n  <div style=\"width:70%;margin:0 auto;\">\n    <div>\n      <el-button type=\"primary\" icon=\"el-icon-back\" style=\"margin-top: 20px\" @click=\"$router.back()\">返回</el-button>\n    </div>\n    <div style=\"margin-top:20px\">\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\n        <el-breadcrumb-item :to=\"{ path: '/more',query:{path:'null'} }\">黑盘</el-breadcrumb-item>\n        <el-breadcrumb-item v-for=\"(item,index) in records\" :key=\"index\">{{item}}</el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n    <div style=\"display: flex;width: 100%;height: auto;flex-wrap: wrap;box-shadow: 0 0 8px rgba(128,128,128,0.51);border-radius: 1.5em;margin-top:20px;padding:30px 50px\">\n      <div v-for=\"(item,index) in currentPack\" :key=\"index\" @click=\"openPack(item)\" style=\"width:10%;height: auto;text-align: center;margin:30px 15px;padding:15px 20px;border-radius:1em;cursor: pointer;transition: all 600ms\" class=\"file\" :title=\"item.fileName+'.'+item.type\" @contextmenu.prevent=\"currentFile=item;fileManage=true;\">\n        <img :src=\"getType(item.type)\" style=\"width:80%\"/>\n        <span style=\"font-size:16px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;width:100%;display: inline-block;margin-top:8px\">{{item.fileName}}</span>\n      </div>\n      <div v-if=\"!currentPack\" style=\"text-align: center;width: 100%\">\n        <img src=\"@/assets/images/none.svg\"/>\n        <div style=\"font-weight: bold;font-size: 20px;color: #61b3ec\"><span style=\"color: black\">黑</span>盘里什么也没有</div>\n      </div>\n    </div>\n\n    <div style=\"display: flex;justify-content: right;margin:30px 0\">\n      <el-button type=\"success\" plain style=\"font-weight: bold\" @click=\"addMkdir=true;\">新建文件夹</el-button>\n    </div>\n\n<!--    新增弹窗-->\n    <el-dialog\n      title=\"新建文件夹\"\n      :visible.sync=\"addMkdir\"\n      width=\"30%\"\n      center>\n      <el-form label-width=\"80px\">\n        <el-form-item label=\"文件名称\">\n          <el-input v-model=\"mkdirName\"></el-input>\n        </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n    <el-button @click=\"addMkdir = false\">取 消</el-button>\n    <el-button type=\"primary\" @click=\"addMkdir = false;newMkdir()\">确 定</el-button>\n    </span>\n    </el-dialog>\n    <!--    新增弹窗-->\n    <!--    文件操作弹窗-->\n    <el-dialog\n      title=\"文件管理\"\n      :visible.sync=\"fileManage\"\n      width=\"36%\"\n      center>\n      <div style=\"display:flex;width:80%;height: 200px;text-align: center;margin:36px auto 0 auto;\" v-if=\"currentFile\">\n        <div>\n          <img :src=\"getType(currentFile.type)\" style=\"height:100%\"/>\n        </div>\n        <div style=\"text-align: left;display: flex;flex-flow: column;justify-content: space-between;height: 160px;margin-top:20px\">\n          <div style=\"font-size: 18px\"><strong>文件名:</strong>&nbsp;{{currentFile.fileName}}</div>\n          <div style=\"font-size: 18px\"><strong>类型:</strong>&nbsp;{{currentFile.type}}</div>\n          <div style=\"font-size: 18px\"><strong>路径<br></strong>黑盘/{{currentFile.path.substring(currentFile.mainPath.length+1)}}</div>\n          <div style=\"font-size: 18px\"><strong>大小<br></strong>{{currentFile.size/1024}}KB&nbsp,&nbsp约{{(parseFloat(currentFile.size)/1024/1024).toFixed(2)}}MB</div>\n        </div>\n      </div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"info\" @click=\"fileManage = false\">取 消</el-button>\n        <el-button type=\"danger\" @click=\"fileManage = false;deleteFile()\">删除</el-button>\n        <el-button type=\"success\" @click=\"fileManage = false;OnlineFile()\" :disabled=\"!(currentFile.type==='txt'||currentFile.type==='jpg'||currentFile.type==='png'||currentFile.type==='xml')\" title=\"仅支持txt,jpg,png格式,不支持中文名\">预览</el-button>\n        <el-button type=\"primary\"  @click=\"fileManage = false;fileDown(currentFile)\">下载</el-button>\n       </span>\n    </el-dialog>\n    <!--    文件操作弹窗-->\n<!--    下载进度条-->\n    <div v-if=\"$store.state.uploadSize\" style=\"position: fixed;z-index: 100;top:30px;width:300px;height: auto;right: 30px;background-color: white;border-radius: .6em;box-shadow: 0 0 5px grey;padding:20px 10px\">\n      <div style=\"font-size: 18px;font-weight: bold;margin:0 auto 20px auto;text-align: center\">{{downFile.fileName}}.{{downFile.type}} &nbsp;{{speed}}</div>\n      <el-progress  style=\"margin:0 auto\" :text-inside=\"true\" :stroke-width=\"36\" :percentage=\"$store.state.uploadSize\" status=\"success\" ></el-progress>\n      <div style=\"color: crimson;text-align: center;margin-top:20px\"><strong>请不要跳转或刷新页面!</strong></div>\n    </div>\n<!--    下载进度条-->\n  </div>\n</template>\n\n<script>\nimport {deleteFile, down, newMkdir, showPack} from \"../net/home\";\nexport default {\n  name: \"more\",\n  data() {\n    return {\n      currentPack:'',\n      currentFile:'',\n      downFile:'',\n      speed:0,\n      oldSize: {\n        size: 0,\n        time: 0\n      },\n      newSize: {\n        size: 0,\n        time: 0\n      },\n      currentPath:\"null\",\n      records:[],\n      fileManage:false,\n      addMkdir: false,\n      mkdirName:'Black盘临时文件夹'\n    };\n  },\n  watch:{\n    '$route.query.path':function(data){\n        this.showPack(data);\n        this.currentPath=this.$route.query.path;\n        sessionStorage.setItem(\"records\",JSON.stringify(this.records));\n        if(data===\"null\"){\n          this.records=[];\n        }\n    },\n    '$store.state.uploadSize': function (data) {\n      this.newSize.size = this.$store.state.uploadReading;\n      this.newSize.time = new Date().getTime();\n      if (this.oldSize.time !== 0) {\n        let speed = ((this.newSize.size - this.oldSize.size) / ((this.newSize.time - this.oldSize.time) / 1000) / 1024).toFixed(2);\n        if (speed > 1024) {\n          this.speed = (speed / 1024).toFixed(2) + \" MB\"\n        } else {\n          this.speed = speed + \" KB\"\n        }\n        if (data >= 100) {\n          this.$store.state.uploadSize = 0;\n          this.$store.commit(\"tip\", {code: \"200\", message: \"文件下载完成\"})\n          this.oldSize = {\n            size: 0,\n            time: 0\n          };\n          this.newSize = {\n            size: 0,\n            time: 0\n          }\n        }\n      }\n      this.oldSize.size = this.newSize.size;\n      this.oldSize.time = this.newSize.time;\n    }\n  },\n  methods: {\n    async showPack(path) {\n      this.currentPack=(await showPack({\n        \"path\":path\n      })).data;\n    },\n    async newMkdir(){\n      let res=await newMkdir({\n        path:this.currentPath,\n        fileName:this.mkdirName\n      });\n      this.$store.commit(\"tip\",res);\n      if(res.code===\"200\"){\n        await this.showPack(this.$route.query.path);\n      }\n    },\n    async openPack(item){\n      if(item.type===\"pack\"){\n        this.records.push(item.fileName);\n        await this.$router.push({\n          path:\"/more\",\n          query:{path:item.path}\n        });\n      }else{\n        this.currentFile=item;\n        this.fileManage=true;\n      }\n    },\n    fun(){\n      this.records.pop();\n    },\n    getType(type){\n      try {\n        return require(\"@/assets/images/fileType/\" + type + \".svg\");\n      } catch (e) {\n        return require(\"@/assets/images/fileType/null.svg\");\n      }\n    },\n    async fileDown(item){\n      if(this.$store.state.uploadSize!==0){\n        this.$store.commit(\"tip\",{code:\"500\",message:\"当前任务正在进行\"})\n      }else {\n        this.downFile = item;\n        let res = await down(item);\n        const content = res\n        const blob = new Blob([content])\n        let fileName;\n        if (item.type === \"pack\" || item.size > 1024*1024*50) {\n          fileName = item.fileName + \".zip\"\n        } else {\n          fileName = item.fileName + \".\" + item.type;\n        }\n        if ('download' in document.createElement('a')) { // 非IE下载\n          const elink = document.createElement('a')\n          elink.download = fileName\n          elink.style.display = 'none'\n          elink.href = URL.createObjectURL(blob)\n          document.body.appendChild(elink)\n          elink.click()\n          URL.revokeObjectURL(elink.href) // 释放URL 对象\n          document.body.removeChild(elink)\n        } else { // IE10+下载\n          navigator.msSaveBlob(blob, fileName)\n        }\n        this.$message({\n          type: 'success',\n          message: '正在下载!'\n        });\n      }\n    },\n    OnlineFile(){\n      let onlinePath=\"https://blackjun.cn/blackYun\"+\"/upload/\"+this.currentFile.path.substring(this.currentFile.mainPath.length+1);\n      window.open(onlinePath, '_blank');\n    },\n    async deleteFile(){\n      let res=await deleteFile(this.currentFile);\n      this.$store.commit(\"tip\",res);\n      if(res.code===\"200\"){\n        await this.showPack(this.$route.query.path);\n      }\n    }\n  },\n  computed:{\n\n  },\n  async created(){\n    await this.showPack(this.$route.query.path);\n    let temp=sessionStorage.getItem(\"records\")\n    if(temp){\n      this.records=JSON.parse(temp);\n      sessionStorage.removeItem(\"records\");\n    }\n  },\n  mounted(){\n    if (window.history && window.history.pushState) {\n      history.pushState(null, null, document.URL);\n      window.addEventListener('popstate', this.fun);//false阻止默认事件\n    }\n  },\n}\n</script>\n\n<style scoped>\n.file:hover{\n  box-shadow: 0 0 10px grey;\n}\n</style>\n"]}]}