UAAU,EAAE,KAHP;MAILO,SAAS,EAAE,KAJN;MAKL7C,KAAK,EAAE,CALF;MAMLD,OAAO,EAAE;QACPP,IAAI,EAAE,CADC;QAEPI,IAAI,EAAE;MAFC,CANJ;MAULL,OAAO,EAAE;QACPC,IAAI,EAAE,CADC;QAEPI,IAAI,EAAE;MAFC;IAVJ,CAAP;EAeF;;AAhGa,CAAf","names":["name","watch","data","newSize","size","$store","state","uploadReading","time","Date","getTime","oldSize","speed","toFixed","uploadSize","commit","code","message","methods","fileDown","item","downFile","res","down","path","filepath","fileName","type","mainPath","content","blob","Blob","document","createElement","elink","download","style","display","href","URL","createObjectURL","body","appendChild","click","revokeObjectURL","removeChild","navigator","msSaveBlob","$message","activeFile","fileManage","goHome","console","log","window","open","downSize","fileBlack"],"sourceRoot":"","sources":["D:\\BlackJun\\2021\\program\\webstrom\\yun\\src\\views\\simple.vue"],"sourcesContent":["<template>\n  <div>\n    <div style=\"width:70%;margin:30px auto 0 auto\">\n      <div style=\"text-align: center\">\n        <strong style=\"font-size: 40px;letter-spacing: 10px;\">轻轻松松<span style=\"color: #3a8ee6;font-size: 60px\">下</span>,简简单单<span\n          style=\"color: #61b3ec;font-size: 50px\">用</span></strong>\n        <div style=\"text-align: center;font-size: 16px;color: #a7adad;margin-top:25px\"><strong>在错综复杂的web环境下,致力于轻轻松松下载minimalist软件,软件持续更新中...欢迎大家留言推荐</strong>\n        </div>\n      </div>\n      <div style=\"margin-top:30px\">\n        <div>\n          <div v-for=\"(item1,index1) in simple\" :key=\"index1\">\n            <img :src=\"item1.icon\" style=\"height: 20px\">\n            <strong style=\"font-size:30px\"\n                    :style=\"item1.classColor\">{{ item1.class }}</strong>&nbsp&nbsp{{ item1.title }}\n            <hr style=\"position:relative;top:10px\">\n            <div style=\"margin-top:10px;display: flex;width: 100%;height: auto;flex-wrap: wrap;\">\n              <div v-for=\"(item2,index2) in item1.programs\" :key=\"index2\"\n                   style=\"width:27%;height: auto;text-align: center;margin:30px 15px;padding:15px 15px;border-radius:1em;cursor: pointer;transition: all 600ms\"\n                   class=\"file\" :title=\"item2.name\" @click=\"activeFile(item2)\">\n                <div style=\"display: flex;\">\n                  <div style=\"flex: none\">\n                    <div style=\"height: 100%;display: table\">\n                      <div\n                        style=\"align-items: center;display: table-cell;vertical-align: middle;height: auto;flex:none\">\n                        <img :src=\"item2.icon\" style=\"width:106px;border-radius: 1em;\"/>\n                        <div>\n                          <span\n                            style=\"font-size:16px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;width:100%;display: inline-block;margin-top:8px;font-weight: bold\">{{ item2.name }}</span>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                  <div\n                    style=\"margin:5px 0 0 20px;height: 130px;display: flex;justify-content: space-between;flex-flow: column;text-align: left\">\n                    <div>\n                      <strong\n                        style=\"background-color: #67c23a;color: white;border-radius: .3em;padding:2px 10px\">优点</strong>\n                      <div style=\"margin-top:5px\">{{ item2.advantage }}</div>\n                    </div>\n                    <div>\n                      <strong\n                        style=\"background-color: rgba(128,128,128,0.42);color: white;border-radius: .3em;padding:2px 10px\">不足</strong>\n                      <div style=\"margin-top:5px\">{{ item2.shortcoming }}</div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!--    文件操作弹窗-->\n    <el-dialog\n      title=\"文件管理\"\n      :visible.sync=\"fileManage\"\n      width=\"36%\"\n      center>\n      <div style=\"flex: none\">\n        <div style=\"height: 100%;display: table;margin:0 auto\">\n          <div\n            style=\"align-items: center;display: table-cell;vertical-align: middle;height: auto;flex:none;text-align: center\">\n            <img :src=\"downFile.icon\" style=\"width:106px;border-radius: 1em;\"/>\n            <div>\n              <span\n                style=\"font-size:16px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;width:100%;display: inline-block;margin-top:8px;font-weight: bold\">{{ downFile.name }}</span>\n            </div>\n          </div>\n        </div>\n      </div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"info\" @click=\"fileManage = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"fileManage = false;goHome()\" :disabled=\"downFile.path==='null'\"\n                   title=\"部分软件没有找到官网\">官网下载</el-button>\n        <el-button type=\"success\" @click=\"fileManage = false;fileDown(downFile)\" :disabled=\"downFile.filepath==='null'\"\n                   title=\"软件官网下载更快\">直接下载</el-button>\n       </span>\n    </el-dialog>\n    <!--    文件操作弹窗-->\n    <!--    下载进度条-->\n    <div v-if=\"$store.state.uploadSize\"\n         style=\"position: fixed;z-index: 100;top:30px;width:300px;height: auto;right: 30px;background-color: white;border-radius: .6em;box-shadow: 0 0 5px grey;padding:20px 10px\">\n      <div style=\"font-size: 18px;font-weight: bold;margin:0 auto 20px auto;text-align: center\">{{ downFile.name }}.zip\n        &nbsp {{ speed }}\n      </div>\n      <el-progress style=\"margin:0 auto\" :text-inside=\"true\" :stroke-width=\"36\" :percentage=\"$store.state.uploadSize\"\n                   status=\"success\"></el-progress>\n      <div style=\"color: crimson;text-align: center;margin-top:20px\"><strong>请不要跳转或刷新页面!</strong></div>\n    </div>\n    <!--    下载进度条-->\n  </div>\n</template>\n\n<script>\n\n\nexport default {\n  name: \"simple\",\n  watch: {\n    '$store.state.uploadSize': function (data) {\n      this.newSize.size = this.$store.state.uploadReading;\n      this.newSize.time = new Date().getTime();\n      if (this.oldSize.time !== 0) {\n\n        let speed = ((this.newSize.size - this.oldSize.size) / ((this.newSize.time - this.oldSize.time) / 1000) / 1024).toFixed(2);\n        if (speed > 1024) {\n          this.speed = (speed / 1024).toFixed(2) + \" MB\"\n        } else {\n          this.speed = speed + \" KB\"\n        }\n        if (data >= 100) {\n          this.$store.state.uploadSize = 0;\n          this.$store.commit(\"tip\", {code: \"200\", message: \"文件下载完成\"})\n          this.oldSize = {\n            size: 0,\n            time: 0\n          };\n          this.newSize = {\n            size: 0,\n            time: 0\n          }\n        }\n      }\n      this.oldSize.size = this.newSize.size;\n      this.oldSize.time = this.newSize.time;\n    }\n  },\n  methods: {\n    async fileDown(item) {\n      if (this.$store.state.uploadSize !== 0) {\n        this.$store.commit(\"tip\", {code: \"500\", message: \"当前任务正在进行\"})\n      } else {\n        this.downFile = item;\n        let res = await down({\n          path: item.filepath,\n          fileName: item.name,\n          type: \"zip\",\n          mainPath: \"/usr/blackjun/blackYun\",\n          size: item.size\n        });\n        const content = res\n        const blob = new Blob([content])\n        let fileName = item.name + \".zip\";\n        if ('download' in document.createElement('a')) { // 非IE下载\n          const elink = document.createElement('a')\n          elink.download = fileName\n          elink.style.display = 'none'\n          elink.href = URL.createObjectURL(blob)\n          document.body.appendChild(elink)\n          elink.click()\n          URL.revokeObjectURL(elink.href) // 释放URL 对象\n          document.body.removeChild(elink)\n        } else { // IE10+下载\n          navigator.msSaveBlob(blob, fileName)\n        }\n        this.$message({\n          type: 'success',\n          message: '正在下载!'\n        });\n      }\n    },\n    activeFile(item) {\n      if (this.$store.state.uploadSize === 0) {\n        this.downFile = item;\n      }\n      this.fileManage = true;\n    },\n    goHome() {\n      if (this.$store.state.uploadSize === 0) {\n        console.log(11);\n        window.open(this.downFile.path, '_blank')\n      } else {\n        this.$store.commit(\"tip\", {code: \"500\", message: \"当前任务正在进行\"})\n      }\n    }\n  },\n  data() {\n    return {\n      downFile: '',\n      downSize: '',\n      fileManage: false,\n      fileBlack: false,\n      speed: 0,\n      oldSize: {\n        size: 0,\n        time: 0\n      },\n      newSize: {\n        size: 0,\n        time: 0\n      },\n    }\n  }\n}\n</script>\n\n<style scoped>\n.file:hover {\n  box-shadow: 0 0 10px grey;\n}\n</style>\n"]}]}